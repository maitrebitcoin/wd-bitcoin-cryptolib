    name : BTN_VérifierSignature
    identifier : 0x2e490e0304f440dd
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         SI SAI_CléPublique=""    ALORS RETOUR
         SI SAI_Signature=""      ALORS RETOUR
         SI SAI_MessageASigner="" ALORS RETOUR
         
         // séparateur RC selon la plateforme
         RC_PLATEFROME est chaine = RC
         SI EnModeLinux() ALORS
         	RC_PLATEFROME = Caract(10)
         FIN
         
         // récup du message a vérifier
         sMessage est buffer = SAI_MessageASigner
         
         // récup clé publique
         cléPublique est Point256
         sCléPrivee est chaine = SAI_CléPublique
         cléPublique.X.affecteAvecChaineHexa( ExtraitChaîne(sCléPrivee, 1,RC_PLATEFROME))
         cléPublique.Y.affecteAvecChaineHexa( ExtraitChaîne(sCléPrivee, 2,RC_PLATEFROME))
         
         // récup signature
         signature est SignatureEcdsa
         signature.R.affecteAvecChaineHexa( ExtraitChaîne(SAI_Signature, 1,RC_PLATEFROME))
         signature.S.affecteAvecChaineHexa( ExtraitChaîne(SAI_Signature, 2,RC_PLATEFROME))
         
         // vérification
         soit (ok, sErreur) = ECDSA_Bitcoin.vérifieSignatureBuffer( sMessage, cléPublique, signature)
         IMG_Signature_ERR..Visible = pas ok
         IMG_Signature_OK..Visible  = ok
         si pas ok ALORS
         	erreur(sErreur)
         FIN
         
         MoiMême..Grisé= Vrai
         
        type : 18
      -
        type : 161
        enabled : false
