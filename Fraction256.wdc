   -
     name : estEgalAModulo
     procedure_id : 1979356633517433611
     type_code : 12
     code : |1+
      // test si A/X = B/Y (mod N)
      procédure estEgalAModulo( autreFraction Fraction256, corps est CorpsModulaire ) : booléen
      
      si dénominateur.estEgalA( autreFraction.dénominateur) ALORS
      	renvoyer numérateur.estEgalA( autreFraction.numérateur )
      FIN
      SI numérateur.estEgalA( autreFraction.numérateur ) ALORS
      	RENVOYER dénominateur.estEgalA( autreFraction.dénominateur )
      FIN
      
      
      // egaux ssi
      // A*Y = B*X (mod N)
      X1 est un entier256 = corps.multiplicationModulo(dénominateur, autreFraction.numérateur )
      X2 est un entier256 = corps.multiplicationModulo(numérateur,   autreFraction.dénominateur )
      renvoyer  X1.estEgalA( X2 )
     type : 458752
