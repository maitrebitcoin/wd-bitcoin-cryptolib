     name : VersEntier256
     procedure_id : 1979622917203021384
     type_code : 12
     code : |1-
      // convertion en entier 256
      // ie effecture la divisision mobulaire. 
      // NB : assez lent
      PROCÉDURE VersEntier256(  corps est CorpsModulaire ) : Entier256 
      dbgAssertion( pas dénominateur.estEgalAZero())
      
      // N/1 = N
      SI :dénominateur.estEgalA1() = 1 ALORS
      	RENVOYER :numérateur
      FIN
      // calcul de l'inversion modulaire modulo P
      inverseModulaireDénominateur est Entier256 = corps.inverseModulo(:dénominateur)
      
      // renvoie N * inv(D)
      renvoyer corps.multiplicationModulo( :numérateur, inverseModulaireDénominateur)
     type : 458756
